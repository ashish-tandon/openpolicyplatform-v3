<script>
    var CludoSearch;
    (function () {
        var cludoSettings = {
            customerId: 10000169,
            engineId: 10000207,

            language: 'en',
            // IDs with input box inside it (custom since we have multiple searches) searchModalBinding shoudl be first 
            searchInputs: ["searchModalBinding", "search-box", "search-box-popup","search-box-nav","search-idliketo"],
            type: 'standardOverlay',
            theme: {
                themeColor: '#1462a6',
                themeBannerColor: { textColor: '#ffffff', backgroundColor: '#1462a6' },
                borderRadius: 0, borderRadiusInput: 0
            },
            searchApiUrl: 'https://api-us1.cludo.com/api/v3',
        };
        CludoSearch = new Cludo(cludoSettings);
        CludoSearch.init();
    })();
</script>

