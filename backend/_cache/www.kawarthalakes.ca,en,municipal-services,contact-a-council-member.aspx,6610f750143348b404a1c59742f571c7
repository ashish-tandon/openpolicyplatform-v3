    $(function() {
		if (/Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent)) {
		    $("#Share a.ShareLink").click(function (event) {
		        if (iCreateObject.isSiteInICreateMode) {
		            showNAMessage();
