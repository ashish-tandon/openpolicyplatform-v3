                                      <div  class="header__desktop-menu-item menu-item menu-item--expanded">
          <div x-data="{
            flyoutMenuOpen: false,
            keydownCb: null,
            tabKeypressHandlerSetup() {
              this.keydownCb = (e) => {
                const KEYCODE_TAB = 9;
                if (e.key !== 'Tab' && e.keyCode !== KEYCODE_TAB) {
                  return;
                }

                const menuItemContainer = e.target.closest('.menu-item');

                if (menuItemContainer) {
                  const itemsContainer = menuItemContainer.querySelector('.header__desktop-menu-container')

                  if (itemsContainer) {
                    const items = itemsContainer.querySelectorAll('a, button');

                    if (
                      (e.shiftKey && document.activeElement === items[0])
                      || (!e.shiftKey && document.activeElement === items[items.length - 1])
                    ) {
                      this.flyoutMenuOpen = false;
                      this.tabKeypressHandlerRemove();
                    }
                  }
                }
              };

              window.addEventListener('keydown', this.keydownCb);
            },
            tabKeypressHandlerRemove() {
              if (typeof this.keydownCb === 'function') {
                window.removeEventListener('keydown', this.keydownCb);
              }
            },
            setupListeners(container, dispatch) {
              const SPACE = 32;
              const ENTER = 13;

              container.querySelectorAll('.js-main-menu-first-level-item')
                .forEach(item => {
                  item.addEventListener('keydown', (e) => {
                    if ([ENTER, SPACE].includes(e.which) || ['Enter', 'Space'].includes(e.key)) {
                      e.preventDefault();
                      e.stopPropagation();
                      dispatch('close-flyout');
                      this.flyoutMenuOpen = true;

                      const menuLinkContainer = e.target.closest('.js-link-container');

                      setTimeout(() => menuLinkContainer.querySelector('.js-flyout-container a').focus());
                    }

                    this.handleLeftRightArrows(e, dispatch);
                  });
                });

              const allContainerLinks = container.querySelectorAll('a');
              allContainerLinks.forEach(link => link.addEventListener('keydown', (e) => {
                if (this.flyoutMenuOpen) {
                  this.handleUpDownArrows(e, [...allContainerLinks], dispatch);
                }
              }));
            },

            handleUpDownArrows(e, allContainerLinks, dispatch) {
              const ARROW_UP = 38;
              const ARROW_DOWN = 40;

              if ([ARROW_UP, ARROW_DOWN].includes(e.which) || ['ArrowUp', 'ArrowDown'].includes(e.key)) {
                let nextItem = null;
                let currentLinkIdx = allContainerLinks.findIndex(el => el === e.target);

                if ([ARROW_UP].includes(e.which) || ['ArrowUp'].includes(e.key)) {
                  nextItem = currentLinkIdx > 0
                    ? allContainerLinks[currentLinkIdx - 1]
                    : allContainerLinks[allContainerLinks.length - 1];
                }

                if ([ARROW_DOWN].includes(e.which) || ['ArrowDown'].includes(e.key)) {
                  nextItem = currentLinkIdx < allContainerLinks.length - 1
                    ? allContainerLinks[currentLinkIdx + 1]
                    : allContainerLinks[0];
                }

                if (nextItem) {
                  nextItem.focus();
                }
              }
            },

            handleLeftRightArrows(e, dispatch) {
              const ARROW_LEFT = 37;
              const ARROW_RIGHT = 39;

              if ([ARROW_LEFT, ARROW_RIGHT].includes(e.which) || ['ArrowLeft', 'ArrowRight'].includes(e.key)) {
                let nextItem = null;
                dispatch('close-flyout');

                if ([ARROW_LEFT].includes(e.which) || ['ArrowLeft'].includes(e.key)) {
                  nextItem = e.target.closest('.menu-item').previousElementSibling;

                  if (!nextItem) {
                    const menuItems = e.target.closest('.block--main').children;
                    nextItem = menuItems[menuItems.length - 1];
                  }
                }

                if ([ARROW_RIGHT].includes(e.which) || ['ArrowRight'].includes(e.key)) {
                  nextItem = e.target.closest('.menu-item').nextElementSibling;

                  if (!nextItem) {
                    nextItem = e.target.closest('.block--main').children[0];
                  }
                }

                if (nextItem) {
                  nextItem.querySelector('.js-main-menu-first-level-item').focus();
                }
              }
            },
          }"
          @keyup.away="flyoutMenuOpen = false"
          @close-flyout.window="flyoutMenuOpen = false; tabKeypressHandlerRemove()"
