<script>
  const carousel21 = new Glide('.grid-image-gallery-carousel-21', {
    type: "carousel",
    perView: 1,
    focusAt: "center",
    autoplay: 0,
    hoverpause: "true",
    perTouch: "false",
    rewind: "true",
    breakpoints: "0",
    classes: {activeNav: "glide__bullet--active-#000000"}
  });

  const thumbnails21 = new Glide('.grid-image-gallery-carousel__thumbnails-21', {
        type: "carousel",
    focusAt: "center",
    perView: 11,
        breakpoints: {
      1280: {
        perView: 8
      },
      768: {
        perView: 6
      },
      448: {
        perView: 4
      }
    },
    classes: {activeNav: "glide__bullet--active-#000000"}
  });

  const glideCarouselInit21 = (el) => {
    if (el !== undefined) {
      carousel21.mount().update({startAt: el});
      thumbnails21.mount().update({startAt: el})

      Array.from(document.querySelectorAll('.grid-image-gallery-carousel__thumbnail-21')).forEach(el => {
        el.addEventListener('click', function () {
          carousel21.go('=' + (el.dataset.index))
          thumbnails21.go('=' + (el.dataset.index))
        })
      })
    }
  }

  const glideCarouselDestroy21 = () => {
    carousel21.destroy();
    thumbnails21.destroy();
  }

  carousel21.on('swipe.end', function () {
    thumbnails21.go('=' + (carousel21.index))
  });

  carousel21.on('move.after', function () {
    thumbnails21.go('=' + (carousel21.index))
  });
</script>

<div
  class="grid-image-gallery-carousel grid-image-gallery-carousel-21 max-w-full bg-darkBlue text-white font-bold">
  <div class="grid-image-gallery-carousel-container">
        <div
      x-data="{
        carouselModal21 : false,
        dataIndex21 : 0,
      }">
      <template
        @carousel-modal.window="
          carouselModal21 = $event.detail.carouselModal21;
          dataIndex21 = $event.detail.dataIndex21;
          console.log($event.detail.dataIndex21);
          $nextTick(() => {($event.detail.dataIndex21 !== undefined) ? glideCarouselInit21($event.detail.dataIndex21) : null});"
        x-if="carouselModal21">
        <div x-transition:enter="transition ease-out duration-300"
             x-transition:enter-start="opacity-0 transform scale-90"
             x-transition:enter-end="opacity-100 transform scale-100"
             x-transition:leave="transition ease-in duration-300"
             x-transition:leave-start="opacity-100 transform scale-100"
             x-transition:leave-end="opacity-0 transform scale-90"
             class="carousel-overlay p-2 fixed h-100 inset-0 z-50 overflow-hidden flex justify-center items-center bg-white container mx-auto">
          <div
            @click.away="
            carouselModal21 = false;
            $dispatch('closed-modal', { showModal21: carouselModal21 });
            $nextTick(() => glideCarouselDestroy21);
            document.body.classList.remove('modal-gallery-opened')"
            class="flex flex-col max-h-full overflow-auto">
            <div class="close-button z-5 absolute">
              <button
                class="outline-none focus:outline-none bg-brightBlue p-2 rounded"
                @click="
                carouselModal21 = false;
                $dispatch('closed-modal', { showModal21: carouselModal21 });
                $nextTick(() => glideCarouselDestroy21);
                document.body.classList.remove('modal-gallery-opened')"
                aria-label="Close">
                  <svg class="fill-current text-white"  xmlns="http://www.w3.org/2000/svg" width="14" height="14" viewBox="0 0 14 14" fill="none">
    <g clip-path="url(#clip0_3297_158)">
      <path fill-rule="evenodd" clip-rule="evenodd" d="M-0.000976562 1.34696L1.34635 -0.000366211L7.00177 5.65505L12.6538 0.00301379L14.0011 1.35034L8.3491 7.00238L14.0011 12.6544L12.6538 14.0017L7.00177 8.34971L1.34635 14.0051L-0.000976562 12.6578L5.65444 7.00238L-0.000976562 1.34696Z" fill="white"/>
    </g>
    <defs>
