      <div>
    


<div id="block-header" class="block--header-with-utility-simple">
  
    
  
    
    
    







<div
  class="header-with-utlity-menu relative xl:h-auto"
  x-data="{
    mobileMenuOpen: false,
    searchOpen: false,
    searchOpenMobile: false,
    showInteractiveElementsCb: null,
    trapFocusKeydownEventListenerCb: null,
    stopScrolling() {
      document.querySelector('body').classList.add('stop-scrolling');
    },
    enableScrolling() {
      document.querySelector('body').classList.remove('stop-scrolling');
    },
    openMobileMenu() {
      this.searchOpen = false;
      this.mobileMenuOpen = true;

      const firstFocusableEl = document.querySelector('.js-close-mobile-menu');
      const lastFocusableEl = document.querySelector('.js-translate-button');

      const items = [
        firstFocusableEl,
        ...document.querySelectorAll('.button--level-1'),
        ...document.querySelectorAll('.js-utility-menu a, .js-utility-menu button'),
        document.querySelector('.js-level-access'),
        document.querySelector('.js-high-contrast'),
        lastFocusableEl
      ];

      this.openTrapElement(firstFocusableEl, lastFocusableEl, items, () => this.closeMobileMenu());
    },
    closeMobileMenu() {
      this.mobileMenuOpen = false;
      this.closeTrapElement();
    },
    openSearchModal(isMobile = true) {
      this.closeMobileMenu();
      this.stopScrolling();

      let firstFocusableEl;
      let wrapper;

      if (isMobile) {
        wrapper = document.querySelector('.js-search-mobile-wrapper');
        firstFocusableEl = document.querySelector('.js-search-mobile-close');
        this.searchOpenMobile = true;
      } else {
        wrapper = document.querySelector('.js-search-wrapper');
        firstFocusableEl = document.querySelector('.js-search-close');
        this.searchOpen = true;
      }

      const items = [firstFocusableEl, ...wrapper.querySelectorAll('input')];
      this.openTrapElement(firstFocusableEl, items[items.length - 1], items, () => this.closeSearch(isMobile));
    },
    closeSearch(isMobile = true) {
      this.enableScrolling();
      this.searchOpenMobile = false;
      this.searchOpen = false;

      this.closeTrapElement(
        document.querySelector(isMobile ? '.js-search-mobile-open' : '.js-search-open')
      );
    },
    openTrapElement(firstEl, lastEl, items, closeCb) {
      window.setTimeout(() => firstEl.focus(), 10);
      this.showInteractiveElementsCb = this.hideInteractiveElements([...items, ...document.querySelectorAll('[aria-hidden=true]')]);

      this.trapFocusKeydownEventListenerCb = (e) => {
        if (typeof closeCb === 'function') {
          const KEYCODE_ESCAPE = 27;
          if (e.key === 'Escape' || e.keyCode === KEYCODE_ESCAPE) {
            closeCb();
          }
        }

        this.trapFocus(e, firstEl, lastEl, items);
      };

      window.addEventListener('keydown', this.trapFocusKeydownEventListenerCb);
    },
    closeTrapElement(element) {
      if (element instanceof HTMLElement) {
        setTimeout(() => element.focus(), 0);
      }

      if (typeof this.trapFocusKeydownEventListenerCb === 'function') {
        window.removeEventListener('keydown', this.trapFocusKeydownEventListenerCb);
        this.trapFocusKeydownEventListenerCb = null;
      }

      if (typeof this.showInteractiveElementsCb === 'function') {
        this.showInteractiveElementsCb();
        this.showInteractiveElementsCb = null;
      }
    },
    hideInteractiveElements(exceptions) {
      const elements = Array.from(document.querySelectorAll('a, button, input, select, textarea, [tabindex]:not([tabindex=\'-1\'])'))
        .filter(el => !exceptions.includes(el));

      elements.forEach(element => element.setAttribute('aria-hidden', true));

      return () => elements.forEach(element => element.removeAttribute('aria-hidden'));
    },
    slideToggle() {
      let button = event.target;
      let container = event.target.nextElementSibling;
      let parentContainer = button.parentElement.parentElement;

      button.children[0].classList.toggle('hidden')
      button.children[1].classList.toggle('hidden')

      container.querySelectorAll('.link--level-2')
        .forEach(item => container.classList.contains('active')
          ? item.setAttribute('aria-hidden', true)
          : item.removeAttribute('aria-hidden')
        );

      /** Slide down. */
      if(!container.classList.contains('active')) {
        button.setAttribute('aria-expanded', true);
        /** Show the container. */
        container.classList.add('active')
        container.style.height = 'auto'

        /** Get the computed height of the container. */
        var height = container.clientHeight + 'px'

        /** Set the height of the content as 0px, */
        /** so we can trigger the slide down animation. */
        container.style.height = '0px'

        /** Do this after the 0px has applied. */
        /** It's like a delay or something. MAGIC! */
        setTimeout(() => {
          container.style.height = height;

          if (parentContainer.classList.contains('active')) {
            parentContainer.style.height = 'auto';

            setTimeout(() => {
              var parentHeight = parentContainer.children[0].offsetHeight + 'px';
              parentContainer.style.height = parentHeight;
            }, 500);
          }
        }, 0)

      /** Slide up. */
      } else {
        button.setAttribute('aria-expanded', false);
        /** Set the height as 0px to trigger the slide up animation. */
        container.style.height = '0px'

        if (parentContainer.classList.contains('active')) {
          parentContainer.style.height = 'auto';

          setTimeout(() => {
            var parentHeight = parentContainer.children[0].offsetHeight + 'px';
            parentContainer.style.height = parentHeight;
          }, 500);
        }

        /** Remove the `active` class when the animation ends. */
        container.addEventListener('transitionend', () => {
          container.classList.remove('active')
        }, {once: true})
      }
    },
    trapFocus(e, firstFocusableEl, lastFocusableEl, items) {
      const KEYCODE_TAB = 9;
      const KEYCODE_ARROW_DOWN = 40;
      const KEYCODE_ARROW_UP = 38;

      if (e.key === 'Tab' || e.keyCode === KEYCODE_TAB) {
        /* shift + tab */
        if ( e.shiftKey && document.activeElement === firstFocusableEl) {
          lastFocusableEl.focus();
          e.preventDefault();
        }
        /* tab */
        else if (!e.shiftKey && document.activeElement === lastFocusableEl) {
          firstFocusableEl.focus();
          e.preventDefault();
        }
      }

      const isDownArrowKey = e.key === 'ArrowDown' || e.keyCode === KEYCODE_ARROW_DOWN;
      const isUpArrowKey = e.key === 'ArrowUp' || e.keyCode === KEYCODE_ARROW_UP;

      if (isDownArrowKey || isUpArrowKey) {
        e.preventDefault();
        const direction = isDownArrowKey ? 1 : -1;
        let idx = items.indexOf(e.target) + direction;

        if (idx >= items.length) {
          idx = 0;
        } else if (idx < 0) {
          idx = items.length - 1;
        }

        while (idx >= 0 && idx < items.length && items[idx].offsetParent === null) {
          idx += direction;
        }

        items[idx].focus();
      }
    }
  }">

    <div class="w-screen h-screen bg-veryLightGrey absolute inset-0 z-20 flex items-center justify-center hidden js-search-wrapper" :class="{ 'hidden': ! searchOpen }">
