$(document).ready(function() {
  $('.LOpen').click(function(e) {
      e.preventDefault();
      if(!$(this).hasClass('open')) {
        $(this).closest('.L-item').find('.bio').css('height', '100%');
        $(this).addClass('open');
        $(this).text('Less');
      }
      else {
        $(this).closest('.L-item').find('.bio').css('height', '6em');
        $(this).removeClass('open');
        $(this).text('More');
      }
  });
});
</script><script>
