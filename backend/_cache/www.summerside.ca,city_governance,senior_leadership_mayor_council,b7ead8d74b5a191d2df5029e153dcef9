$(document).ready(function() {
var ww = $(window).width();

  /* Facebook responsivness */
	if ($(".facebookFeedW").length) {
			var new_width = $(".socialFeed").width();
			var $iframe = $(".facebookFeedW iframe");
			$iframe.css("width", new_width);
			var url = $iframe.attr('data-src').split("&width=");
			url = url[0] + '&width=' + new_width + '&height=600&small_header=true&adapt_container_width=true&hide_cover=true&show_facepile=true&appId';
			$iframe.attr('src', url);
		}


